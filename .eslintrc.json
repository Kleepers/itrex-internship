{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "func-names": "off",
    "react/prop-types": "off",
    "react/jsx-filename-extension": "off",
    "import/order": "off",
    "import/prefer-default-export": "off",
    "react/no-array-index-key": "off",
    "max-len": ["error",{ "code": 120}],
    "jsx-a11y/anchor-is-valid": "off"
  }
}
